{
  "name": "google-sheets-mcp",
  "version": "1.3.0",
  "description": "Model Context Protocol (MCP) server for Google Sheets API integration",
  "author": "Shakeel Ahmad",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "bin": {
    "google-sheets-mcp": "./dist/index.cjs"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup",
    "start": "node dist/index.cjs",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "check": "npm run typecheck && npm run lint && npm run format:check",
    "check:all": "npm run check && npm run test:run && npm run build",
    "prepublishOnly": "npm run clean && npm run build",
    "inspector": "npx @modelcontextprotocol/inspector node dist/index.cjs",
    "inspector:dev": "NODE_ENV=development npx @modelcontextprotocol/inspector npx tsx src/index.ts"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "google-sheets",
    "spreadsheet",
    "api",
    "claude",
    "ai"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "googleapis": "^140.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "dotenv": "^16.3.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "prettier": "^3.5.3",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^3.1.4"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shakeelbajeeda/google-sheets-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/shakeelbajeeda/google-sheets-mcp.git/issues"
  },
  "homepage": "https://github.com/shakeelbajeeda/google-sheets-mcp.git#readme"
}
